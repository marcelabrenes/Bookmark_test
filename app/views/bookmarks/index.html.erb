<p id="notice"><%= notice %></p>

<h1>Bookmarks</h1>

<div class="m-4">
  <%= render 'form', bookmark: @bookmark %>
</div>

<table class="table" id="table_bookmarks">
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Kind</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
     <tr>
      <%= render @bookmarks %>
     </tr>
  </tbody>
</table>

<br>

<%= link_to 'New Bookmark', new_bookmark_path %>

<%= pie_chart @bookmarks, title: "Markers"%>
